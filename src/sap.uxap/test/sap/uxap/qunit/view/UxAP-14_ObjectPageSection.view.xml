<mvc:View
		xmlns:mvc="sap.ui.core.mvc"
		xmlns="sap.uxap"
		xmlns:sample="sap.uxap.testblocks.multiview"
		xmlns:m="sap.m"
		xmlns:html="http://www.w3.org/1999/xhtml"
		height="100%">

	<ObjectPageLayout id="ObjectPageLayout">
		<sections>
			<ObjectPageSection id="SectionHiddenTitle1" title="My first section title with subsection" showTitle="false">

				<subSections>
					<ObjectPageSubSection id="subsection1">
						<blocks>
							<sample:MultiViewBlock/>
						</blocks>
					</ObjectPageSubSection>

				</subSections>
			</ObjectPageSection>
			<ObjectPageSection id="SectionHiddenTitle2" title="My second section title with subsection" showTitle="false">

				<subSections>
					<ObjectPageSubSection id="subsection2">
						<blocks>
							<sample:MultiViewBlock/>
						</blocks>
					</ObjectPageSubSection>

				</subSections>
			</ObjectPageSection>
		</sections>

	</ObjectPageLayout>

</mvc:View>
