				"description": {
					"value": "{JobResponsibilities}"
				},
				"title": {
					"value": "{JobTitle}"
				},
				"owner": {
					"value": "{Name}"
				},
				"ownerImage": {
					"value": "{Photo}"
				},
				"icon": {
					"src": "{Icon}"
				}
			}
		},
