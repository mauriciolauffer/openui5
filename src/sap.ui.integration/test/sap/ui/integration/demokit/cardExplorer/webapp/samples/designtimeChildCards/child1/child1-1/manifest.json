{
	"_version": "1.18.0",
	"sap.app": {
		"id": "sap.ui5.test.cardeditor.listcard.child1.child1-1",
		"type": "card",
		"i18n": "../../i18n/i18n.properties",
		"title": "Orders",
		"subTitle": "Test Card for Parameters",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "sap-icon://accept"
		}
	},
	"sap.card": {
		"type": "List",
		"configuration": {
			"editor": "dt",
			"parameters": {
				"cardTitle": {
					"value": "{{TRANSLATED_CARD_TITLE_ORDERS}}"
				},
				"string": {
					"value": "StringValue"
				},
				"boolean": {
					"value": false
				},
				"Customer": {
					"value": ""
				},
				"Employee": {
					"value": ""
				}
			},
			"destinations": {
				"string": {
					"name": "Simple"
				},
				"local": {
					"name": "local",
					"label": "local destination label defined in manifest"
				},
				"dest": {
					"name": "Northwind",
					"defaultUrl": "https://services.odata.org/V4/Northwind/Northwind.svc"
				},
				"destLabel": {
					"name": "products",
					"label": "Direct Destination Label defined in manifest"
				},
				"destLabelTrans": {
					"name": "SF",
					"label": "{i18n>TRANSLATED_DESTINATION_LABEL} defined in manifest"
				}
			}
		},
		"header": {
			"title": "{parameters>/cardTitle/value}",
			"icon": {
				"src": "sap-icon://add-document",
				"shape": "Circle",
				"backgroundColor": ""
			}
		},
		"data": {
			"request": {
				"url": "{{destinations.dest}}/Orders",
				"parameters": {
					"$select": "OrderID, OrderDate, CustomerID, EmployeeID",
					"$filter": "(CustomerID eq '{parameters>/Customer/value}') and (EmployeeID eq {parameters>/Employee/value})"
				}
			},
			"path": "/value"
		},
		"content": {
			"item": {
				"title": "{OrderID}",
				"description": "{= ${OrderID} !== undefined ? ${OrderID} + '-' +  ${CustomerID} + '-' + ${EmployeeID} : ''}",
				"info": {
					"value": "{OrderDate}"
				}
			},
			"maxItems": 5
		}
	}
}