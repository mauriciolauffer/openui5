				"src": "sap-icon://add-document",
				"shape": "Circle",
				"backgroundColor": ""
			},
			"status": {
				"text": {
				 	 "format": {
						"translationKey": "i18n>CARD.COUNT_X_OF_Y",
						"parts": [
					  		"activities",
					  		"count"
						]
					}
				}
			}
		},
		"data1": {
			"extension": {
				"method": "getData",
				"args": ["{filters>/country/value}"]
			},
			"path": "/values"
		},
		"data": {
			"request": {
				"url": "{{destinations.northwind}}/Customers",
				"parameters": {
					"$select": "CustomerID, CompanyName, Country, City, Address"
				}
			},
			"path": "/value"
		},
		"content": {
			"item": {
				"title": "{CustomerID}",
				"description": "Trainer: {CompanyName}",
				"info": {
					"value": "Location: {Country} - {City} - {Address}"
				}
			},
			"item1": {
				"title": "{title}",
				"description": "Trainer: {trainer}",
				"info": {
					"value": "Location: {location}"
				}
			},
			"maxItems": "{parameters>/maxItems/value}"
		},
		"footer": {
			"paginator": {
				"totalCount": 80,
				"pageSize": "{parameters>/maxItems/value}"
			}
		}
	}
