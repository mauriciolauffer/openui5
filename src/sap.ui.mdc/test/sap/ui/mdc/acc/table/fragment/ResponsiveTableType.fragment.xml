<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:form="sap.ui.layout.form">
	<Dialog id="respTableTypeDialog" title="ResponsiveTableType Configuration" class="sapUiSmallMargin">
		<content>
		  <form:SimpleForm editable="true" id="configForm">
				<core:Fragment fragmentName="sap.ui.mdc.acc.table.fragment.TableCommonConfig"/>
				<Label text="Popin Display" labelFor="selectPopinDisplay"/>
				<Select id="selectPopinDisplay" items="{tableConfig>/enums/popinDisplay}" selectedKey="{tableConfig>/respType/popinDisplay}">
					<core:Item key="{tableConfig>value}" text="{tableConfig>value}"></core:Item>
				</Select>
		  </form:SimpleForm>
		</content>
		<endButton>
		  <Button id="btnClose" text="Close" press=".closeDialog" />
		</endButton>
	  </Dialog>
</core:FragmentDefinition>