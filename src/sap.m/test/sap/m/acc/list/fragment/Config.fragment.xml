<core:FragmentDefinition 
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<OverflowToolbar>
		<ToolbarSpacer />
		<Label text="Selection Mode" labelFor="selectSelectionMode" showColon="true"/>
		<Select
			id="selectSelectionMode"
			selectedKey="{listConfig>/selectionMode}">
			<items>
				<core:Item key="None" text="None" />
				<core:Item key="SingleSelectLeft" text="SingleSelectLeft" />
				<core:Item key="MultiSelect" text="MultiSelect" />
			</items>
		</Select>
		<Label text="Content" labelFor="selectContent" showColon="true"/>
		<Select
			id="selectContent"
			selectedKey="{listConfig>/content}"
			change="onContentChange">
			<items>
				<core:Item key="Show Data" text="Show Data" />
				<core:Item key="No Data" text="No Data" />
			</items>
		</Select>
		<Label text="Show Row Actions" labelFor="switchShowRowActions" showColon="true"/>
		<Switch
			id="switchShowRowActions"
			state="{listConfig>/showRowActions}" />
	</OverflowToolbar>
</core:FragmentDefinition>