{
	"_version": "1.18.0",
	"sap.app": {
		"id": "sap.ui5.test.editor.listcard.child1.child1-2",
		"type": "card",
		"i18n": "../../../../../i18n/i18n.properties",
		"title": "Test Card for Parameters",
		"subTitle": "Test Card for Parameters",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "sap-icon://accept"
		}
	},
	"sap.card": {
		"type": "List",
		"configuration": {
			"editor": "dt",
			"parameters": {
				"cardTitle": {
					"value": "{{TRANSLATED_CARD_TITLE}}"
				},
				"maxItems": {
					"value": 5
				},
				"Order": {
					"value": "1"
				}
			},
			"destinations": {
				"northwind": {
					"name": "Northwind"
				},
				"mock_request": {
					"name": "mock_request"
				}
			}
		},
		"header": {
			"title": "{parameters>/string/value}",
			"icon": {
				"src": "sap-icon://add-document",
				"shape": "Circle",
				"backgroundColor": ""
			}
		},
		"data": {
			"request": {
				"url": "{{destinations.mock_request}}/Order_Details",
				"parameters": {
					"$filter": "(OrderID eq '{parameters>Order/value}')"
				}
			},
			"path": "/value"
		},
		"content": {
			"item": {
				"title": "{ProductID}",
				"description": "${OrderID} + '-' +  ${ProductID} + ':' + ${Product/ProductName}",
				"info": {
					"value": "${UnitPrice} + ' USD, count: '+ ${Quantity}"
				}
			},
			"maxItems": "{parameters>/maxItems/value}"
		}
	}
}