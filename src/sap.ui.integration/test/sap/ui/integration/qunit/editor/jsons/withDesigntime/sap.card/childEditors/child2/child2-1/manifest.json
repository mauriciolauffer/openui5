{
	"_version": "1.18.0",
	"sap.app": {
		"id": "sap.ui5.test.editor.listcard.child2.child2-1",
		"type": "card",
		"i18n": "../../../../i18n/i18n.properties",
		"title": "Test Card for Parameters",
		"subTitle": "Test Card for Parameters",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "sap-icon://accept"
		}
	},
	"sap.card": {
		"type": "List",
		"configuration": {
			"editor": "dt",
			"parameters": {
				"cardTitle": {
					"value": "Child Card Title Child2-1"
				},
				"string": {
					"value": "StringValue Child2-1"
				}
			},
			"destinations": {
				"northwind": {
					"name": "Northwind"
				},
				"mock_request": {
					"name": "mock_request"
				}
			}
		},
		"header": {
			"title": "{parameters>/cardTitle/value}",
			"icon": {
				"src": "sap-icon://add-document",
				"shape": "Circle",
				"backgroundColor": ""
			}
		},
		"data": {
			"request": {
				"url": "{{destinations.mock_request}}/Customers",
				"parameters": {
					"$select": "CustomerID, CompanyName, Country, City, Address"
				}
			},
			"path": "/value"
		},
		"content": {
			"item": {
				"title": "{CustomerID}",
				"description": "Trainer: {CompanyName}",
				"info": {
					"value": "Location: {Country} - {City} - {Address}"
				}
			},
			"maxItems": 5
		}
	}
}