{
	"_version": "1.81.0",
	"sap.app": {
		"id": "card.explorer.list.card.numeric.header",
		"type": "card",
		"title": "Sample of a List With Numeric Header",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"shortTitle": "A short title for this Card",
		"info": "Additional information about this Card",
		"description": "A long description for this Card",
		"tags": {
			"keywords": [
				"List",
				"Numeric",
				"Header",
				"Card",
				"Sample"
			]
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "sap-icon://list"
		}
	},
	"sap.card": {
		"type": "Object",
		"header": {
			"type": "Numeric",
			"data": {
				"json": {
					"kpiInfos": {
						"kpi": {
							"number": "200",
							"unit": "%",
							"trend": "Up",
							"state": "Good",
							"target": {
								"number": 150,
								"unit": "%"
							},
							"deviation": {
								"number": 25.43
							},
							"details": "Q1, 2019"
						}
					}
				},
				"path": "/kpiInfos/kpi"
			},
			"title": "Project Cloud Transformation",
			"subtitle": "Based on Year-to-Date Performance",
			"unitOfMeasurement": "EUR",
			"mainIndicator": {
				"number": "{number}",
				"unit": "{unit}",
				"trend": "{trend}",
				"state": "{state}"
			},
			"sideIndicators": [
				{
					"title": "Target",
					"number": "{target/number}",
					"unit": "{target/unit}"
				},
				{
					"title": "Deviation",
					"number": "{deviation/number}",
					"unit": "%"
				}
			],
			"details": "{details}"
		},
		"content": {
			"groups": [
				{
					"items": [
						{
							"value": "Content should be provided here."
						}
					]
				}
			]
		}
	}
}