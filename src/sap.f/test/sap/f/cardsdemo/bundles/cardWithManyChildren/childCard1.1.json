{
	"sap.app": {
		"id": "childCard1.1",
		"type": "card",
		"i18n": "i18n/i18n.properties"
	},
	"sap.card": {
		"type": "Table",
		"configuration": {
			"parameters": {
				"firstColumnTitle": {
					"value": "Title"
				}
			},
			"childCards": {
				"someCard": {
					"manifest": "manifest.json"
				}
			}
		},
		"data": {
			"json": {
				"items": [
					{
						"title": "Item 1"
					},
					{
						"title": "Item 2"
					},
					{
						"title": "Item 3"
					}
				]
			}
		},
		"header": {
			"title": "Child Card 1.1",
			"actions": [
				{
					"type": "ShowCard",
					"parameters": {
						"childCardKey": "someCard"
					}
				}
			]
		},
		"content": {
			"data": {
				"path": "/items"
			},
			"row": {
				"columns": [
					{
						"title": "{parameters>/firstColumnTitle/value}",
						"value": "{title}"
					},
					{
						"title": "Description",
						"value": "Description"
					}
				]
			}
		}
	}
}